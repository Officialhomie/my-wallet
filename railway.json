{
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "cd wallet-farm && npm install"
  },
  "deploy": {
    "startCommand": "cd wallet-farm && npm run server",
    "healthcheckPath": "/health",
    "healthcheckTimeout": 300,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}
